	<append xpath="/blocks/block[@name='plantedYucca3HarvestPlayer']">
		<property name="Extends" value="plantedYucca3Harvest" param1="Class"/>
	</append>


<!--  TIRES -->
	<append xpath="/blocks/block[@name='decoCarTireSmallFlat']" >
		<property name="CanPickup" value="true"/>
	</append>
	<append xpath="/blocks/block[@name='decoCarTireFlat']" >
		<property name="CanPickup" value="true"/>
	</append>
	<append xpath="/blocks/block[@name='decoCarTirePile']" >
		<property name="CanPickup" value="true"/>
	</append>
	<append xpath="/blocks/block[@name='decoCarTireStack']" >
		<property name="CanPickup" value="true"/>
	</append>
	<append xpath="/blocks/block[@name='decoCarWheelFlat']" >
		<property name="CanPickup" value="true" param1="vehicleWheels"/>
	</append>


<!-- TRAPS -->
	<append xpath="/blocks/block[@name='trapSpikesNew']" >
		<property name="CreativeMode" value="Player"/>
		<property name="CanPickup" value="true"/>
	</append>
	<append xpath="/blocks/block[@name='trapSpikesWoodDmg0']" >
		<property name="CanPickup" value="true"/>
	</append>
	<append xpath="/blocks/block[@name='trapSpikesIronDmg0']" >
		<property name="CanPickup" value="true"/>
	</append>


<!-- WAGON WHEEL -->
	<append xpath="/blocks/block[@name='decoWildWestWagon']" >
		<property name="CanPickup" value="true"/>
		<property name="Weight" value="40"/>
	</append>
	<append xpath="/blocks/block[@name='decoWildWestWagonWheelBack']" >
		<property name="CanPickup" value="true"/>
		<property name="Weight" value="40"/>
	</append>
</DrsA18PickupEnvironment>